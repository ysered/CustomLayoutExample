<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagView">
        <attr name="tag_text" format="string"/>
        <attr name="tag_background" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TagLayout">
        <attr name="tag_space_horizontal" format="dimension"/>
        <attr name="tag_space_vertical" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="my_view_state">
        <attr name="my_view_default" format="boolean"/>
        <attr name="my_view_selected" format="boolean"/>
    </declare-styleable>
</resources>
